## 模块定义
#module_begin test_list

  ## 类型定义
  #struct_begin node
    value int64
    next reference
  #struct_end

  ## 过程定义
  #proc_begin print
    #params int64
    #code_begin
      ret
    #code_end
  #proc_end

  #proc_begin main
    #locals reference reference reference
    #code_begin
      ## a = new test_list.node
      loadc type "test_list.node"
      new
      storer 0

      ## a.value = 1000
      loadc int64 "1000"
      loadc field "test_list.node.value"
      loadr 0
      setp

      ## a.next = null
      loadn
      loadc field "test_list.node.next"
      loadr 0
      setr

      ## b = new test_list.node
      loadc type "test_list.node"
      new
      storer 1

      ## b.value = 2000
      loadc int64 "2000"
      loadc field "test_list.node.value"
      loadr 1
      setp

      ## b.next = a
      loadr 0
      loadc field "test_list.node.next"
      loadr 1
      setr

      ## head = b
      loadr 1
      storer 2

      ## if (head == null) break
      #label BEGIN
      loadn
      loadr 2
      eq_ref
      if END

      ## print(head.value)
      loadc field "test_list.node.value"
      loadr 2
      getp
      loadc proc "test_list.print"
      call

      ## head = head.next
      loadc field "test_list.node.next"
      loadr 2
      getr
      storer 2

      goto BEGIN

      #label END
      ret
    #code_end
  #proc_end
#module_end
